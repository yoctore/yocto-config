{
  "app" : {
    "name" : "myAppName",
    "stackError" : true,
    "session" : {
      "timeout" : 30
    }
  },
  "express" : {
    "jsonp"       : true,       
    "prettyHtml"  : true,
    "filter"      : {
      "rules" : "json|text|javascript|css|html",
      "by"    : "Content-Type",
      "level" : 9
    },
    "viewEngine" : "jade",
    "session" : {
      "enable" : true,
      "options" : {
        "secret" : "15Octobre2014",
        "name" : "mySessionName",
        "secure" : true,
        "genuuid" : true,
        "proxy" : true,
        "resave" : true,
        "saveUninitialized" : true
      }
    },
    "vhost" : {
      "enable"      : false,
      "options" : {
        "url"         : "myhosturl.url",
        "aliases"     : [ "alias.myhosturl.url" ],      
        "subdomains"  : true,
        "http"        : {
          "redirect" : {
            "type" : 301,
            "url"  : "www.myurl.url",
            "port" : 80
          }
        }
      }
    }
  }
}
