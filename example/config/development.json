{
  "app" : {
    "name" : "my AP DEV"
  },
  "express" : {
    "jsonp"       : false,
    "viewEngine" : "jade",
    "vhost" : {
      "enable"      : true,
      "options" : {
        "url"         : "myhosturl.url",
        "aliases"     : [ "alias.myhosturl.url" ],      
        "subdomains"  : true,
        "http"        : {
          "redirect" : {
            "type" : 301,
            "url"  : "www.myurl.url",
            "port" : 80
          }
        }
      }
    }
  }
}
